{
  "source": {
    "include": ["README.md", "src"],
    "exclude": ["src/assets"],
    "includePattern": ".+\\.js$"
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./out/docs/",
    "recurse": true,
    "verbose": true,
    "prism-theme": "prism-custom",
    "template": "src/assets/docdash"
  },
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ],
  "templates": {
    "monospaceLinks": true
  },
  "metadata": {
    "title": "Traktify API Documentation",
    "logo": "https://i.imgur.com/Y5rSPSm.png"
  },
  "docdash": {
    "sectionOrder": [
      "Tutorials",
      "Classes",
      "Interfaces",
      "Modules",
      "Externals",
      "Events",
      "Namespaces",
      "Mixins"
    ],
    "menu": {
      "Bugs & Issues": {
        "href":"https://github.com/CodingBobby/traktify/issues",
        "target":"_blank",
        "class":"menu-item",
        "id":"issues_link"
      },
      "Build Log": {
        "href": "https://circleci.com/gh/CodingBobby/traktify",
        "target": "_blank",
        "class": "menu-item",
        "id": "build_link"
      },
      "Discord Server": {
        "href":"https://discord.gg/BJNAMcj",
        "target":"_blank",
        "class":"menu-item",
        "id":"discord_link"
      }
    },
    "search": true
  }
}
