{
   "source": {
      "include": ["README.md", "src"],
      "exclude": ["src/assets"],
      "includePattern": ".+\\.js$"
   },
   "opts": {
      "encoding": "utf8",
      "destination": "./out/docs/",
      "recurse": true,
      "verbose": true,
      "prism-theme": "prism-custom",
      "tutorials": "tutorials",
      "template": "src/assets/docdash"
   },
   "plugins": [
      "plugins/markdown",
      "plugins/summarize"
   ],
   "templates": {
      "monospaceLinks": true
   },
   "metadata": {
      "title": "Traktify API Documentation",
      "logo": "https://i.imgur.com/Y5rSPSm.png"
   },
   "docdash": {
      "sectionOrder": [
         "Tutorials",
         "Classes",
         "Interfaces",
         "Modules",
         "Externals",
         "Events",
         "Namespaces",
         "Mixins"
      ],
      "menu": {
         "Project Website": {
            "href":"https://codingbobby.xyz/traktify",
            "target":"_blank",
            "class":"menu-item",
            "id":"website_link"
         },
         "Bugs & Issues": {
            "href":"https://github.com/CodingBobby/traktify/issues",
            "target":"_blank",
            "class":"menu-item",
            "id":"issues_link"
         },
         "Discord Server": {
            "href":"https://discord.gg/BJNAMcj",
            "target":"_blank",
            "class":"menu-item",
            "id":"discord_link"
         }
      }
   }
}
